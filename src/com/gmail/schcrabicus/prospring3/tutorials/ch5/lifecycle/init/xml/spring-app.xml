<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
        default-lazy-init="true">

    <bean id="simpleBean1" class="com.gmail.schcrabicus.prospring3.tutorials.ch5.lifecycle.init.xml.SimpleBean" init-method="init">
        <property name="name">
            <value>Kolya Lushchitskiy</value>
        </property>
        <property name="age">
            <value>100</value>
        </property>
    </bean>

    <bean id="simpleBean2" class="com.gmail.schcrabicus.prospring3.tutorials.ch5.lifecycle.init.xml.SimpleBean" init-method="init">
        <property name="age">
            <value>50</value>
        </property>
    </bean>

    <bean id="simpleBean3" class="com.gmail.schcrabicus.prospring3.tutorials.ch5.lifecycle.init.xml.SimpleBean" init-method="init">
        <property name="name">
            <value>Kolya Lushchitskiy</value>
        </property>
    </bean>

</beans>